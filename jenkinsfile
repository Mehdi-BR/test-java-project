#!groovy
pipeline {
  agent none
  tools{
  jdk '11'}
  
  stages {    
stage('Example'){
  steps{
    sh '''
    env | grep -e PATH -e JAVA_HOME
    which java
    java -version'''
}
 }
     
stage('Maven Install') {
     agent {        
docker {          
image 'maven:3.5.0'
}      
}      
steps {
      sh 'mvn clean install'
      }
    }
   }
}
